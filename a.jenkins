pipeline {
   agent any
 
   stages {
 stage('Setup Environment') {
            steps {
                script {
                    dir('financial-data-processing') {
                        sh 'python3 -m venv venv'
                        sh './venv/bin/pip install -r requirements.txt'
                    }
                }
            }
        }
 
      stage('runpy') {
